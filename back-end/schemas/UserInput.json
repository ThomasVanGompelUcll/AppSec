{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "UserInput",
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string"
      },
      "lastName": {
        "type": "string"
      },
      "age": {
        "type": "number"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "password": {
        "type": "string"
      },
      "phoneNumber": {
        "type": "string"
      },
      "personalNumber": {
        "type": "string",
        "format": "int64"
      },
      "role": {
        "type": "string"
      }
    },
    "required": ["firstName", "lastName", "email", "password", "phoneNumber", "personalNumber", "role"]
  }