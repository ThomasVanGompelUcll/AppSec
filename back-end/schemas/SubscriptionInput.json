{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "SubscriptionInput",
    "type": "object",
    "properties": {
        "description": {
            "type": "string"
        },
        "amount": {
            "type": "number"
        },
        "startDate": {
            "type": "string",
            "format": "date-time"
        },
        "endDate": {
            "type": "string",
            "format": "date-time"
        },
        "expense": {
            "type": "boolean"
        },
        "frequency": {
            "type": "string"
        },
        "currency": {
            "type": "string"
        },
        "walletId": {
            "type": "number"
        },
        "userId": {
            "type": "number"
        }
    },
    "required": ["description", "amount", "startDate", "endDate", "expense", "frequency", "currency", "walletId", "userId"]
}